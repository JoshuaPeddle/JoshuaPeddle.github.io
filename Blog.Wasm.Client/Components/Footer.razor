<MudDivider Class="mt-8 mb-4" />
<MudContainer MaxWidth="MaxWidth.ExtraExtraLarge">
    <MudGrid>
        <MudItem xs="12" sm="6">
            <MudText Typo="Typo.subtitle2">© @DateTime.Now.Year Josh Peddle</MudText>
        </MudItem>
        <MudItem xs="12" sm="6" Class="d-flex justify-end align-center">
            <MudStack Row>
                <MudLink Href="https://github.com/joshuapeddle" Target="_blank">
                    <MudIcon Icon="@Icons.Custom.Brands.GitHub" Size="Size.Medium" />
                </MudLink>
                <MudLink Href="https://exifgone.joshuapeddle.com" Target="_blank">
                    <MudIcon Icon="@Icons.Material.Filled.Camera" Size="Size.Medium" />
                </MudLink>
                <MudLink Href="mailto:contact@example.com" Target="_blank">
                    <MudIcon Icon="@Icons.Material.Filled.Email" Size="Size.Medium" />
                </MudLink>
            </MudStack>
        </MudItem>
    </MudGrid>
</MudContainer>